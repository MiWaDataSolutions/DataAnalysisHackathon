  "components": {
    "schemas": {
      "DataSession": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "schemaName": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "lastUpdatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "userId": {
            "type": "string",
            "nullable": true
          },
          "user": {
            "$ref": "#/components/schemas/User"
          }
        },
        "additionalProperties": false
      },
      "MeDto": {
        "type": "object",
        "properties": {
          "isAuthenticated": {
            "type": "boolean",
            "description": "Whether the user is authenticate or not"
          },
          "user": {
            "$ref": "#/components/schemas/MeUserDto"
          }
        },
        "additionalProperties": false
      },
      "MeUserDto": {
        "type": "object",
        "properties": {
          "googleId": {
            "type": "string",
            "description": "The user's Google Id",
            "nullable": true
          },
          "email": {
            "type": "string",
            "description": "The user's email address",
            "nullable": true
          },
          "name": {
            "type": "string",
            "description": "The user's name",
            "nullable": true
          },
          "profilePictureUrl": {
            "type": "string",
            "description": "The user's profile picture",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": { }
      },
      "User": {
        "required": [
          "email",
          "googleId",
          "name"
        ],
        "type": "object",
        "properties": {
          "googleId": {
            "minLength": 1,
            "type": "string",
            "description": "Gets or sets the user's unique Google identifier.\r\nThis is primarily validated from the token claims on the backend and serves as the Primary Key."
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "description": "Gets or sets the user's email address.\r\nThis is primarily validated from the token claims on the backend.",
            "format": "email"
          },
          "name": {
            "minLength": 1,
            "type": "string",
            "description": "Gets or sets the user's display name.\r\nThis is typically provided in the request body."
          },
          "profilePictureUrl": {
            "type": "string",
            "description": "Gets or sets the URL of the user's profile picture. Optional.",
            "nullable": true
          },
          "createdAtUtc": {
            "type": "string",
            "description": "Gets or sets the UTC date and time when this user record was created in the database.",
            "format": "date-time"
          },
          "lastLoginAtUtc": {
            "type": "string",
            "description": "Gets or sets the UTC date and time when this user record was updated in the database",
            "format": "date-time"
          },
          "userDatabaseConnectionString": {
            "type": "string",
            "description": "Gets or sets the users database conenction string",
            "nullable": true
          }
        },
        "additionalProperties": false,
        "description": "Represents a user's profile information, typically received from the client."
      }
    },
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "description": "JWT Authorization header using the Bearer scheme. Example: \"Authorization: Bearer {token}\"",
        "scheme": "Bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "BearerAuth": [ ]
    }
  ]
